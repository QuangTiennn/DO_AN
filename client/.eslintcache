S/DO_AN/client/src/reducers/tour.js",["367"],"\nimport { history } from '../store'\n\nimport Toastify from '../utils/toastify'\n\nimport * as Types from '../constants/ActionTypeTour';\n\nconst initialState = {\n  listTour: [],\n  isRefresh: false\n}\n\nconst findIndex = (tours, id) => {\n  let result = -1;\n  tours.forEach((tour, index) => {\n    if (tour._id === id) {\n      result = index;\n    }\n  });\n  return result;\n};\n\nconst tours = (state = initialState, action) => {\n  let index = -1;\n  const tourID = action.id;\n  const { tour, isSuccess } = action;\n\n  switch (action.type) {\n    case Types.FETCH_TOURS:\n      return { ...state, listTour: action.tours }\n    case Types.ADD_TOURS:\n      if (isSuccess) {\n        Toastify({ msg: 'Create tour successfully', type: 'success' });\n        state.listTour.push(action.tour);\n      }\n      return state\n    case Types.DELETE_TOURS:\n      index = findIndex(state.listTour, tourID);\n      state.listTour.splice(index, 1);\n      return state\n    case Types.UPDATE_TOURS:\n\n      index = findIndex(state.listTour, action.tour._id);\n      state.listTour[index] = tour;\n      Toastify({ msg: 'Update tour successfully', type: 'success' });\n      return { ...state, isRefresh: true }\n    case Types.SEARCH_TOUR:\n\n      return { ...state, listTour: action.payload }\n\n    default: return state\n  }\n};\n\nexport default tours;\n","/Users/truong.doshi/Coding/React JS/DO_AN/client/src/context/socket.js",[],"/Users/truong.doshi/Coding/React JS/DO_AN/client/src/reducers/currentUser.js",[],"/Users/truong.doshi/Coding/React JS/DO_AN/client/src/actions/actCurrentUser.js",[],"/Users/truong.doshi/Coding/React JS/DO_AN/client/src/reducers/messTemp.js",[],"/Users/truong.doshi/Coding/React JS/DO_AN/client/src/actions/saveMessAction.js",[],{"ruleId":"368","severity":1,"message":"369","line":7,"column":3,"nodeType":"370","messageId":"371","endLine":9,"endColumn":4},{"ruleId":"372","replacedBy":"373"},{"ruleId":"374","replace