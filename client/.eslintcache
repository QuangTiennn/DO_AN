  ));\n      }\n      return result;\n    }\n\n    render() {\n      return (\n        <div>\n          <Header />\n          <Switch>\n            { this.showContentMenu(routes) }\n            {/* <Route exact={true} path=\"/\" component={HomePage} /> */}\n          </Switch>\n          <ChatClient />\n          <Footer />\n        </div>\n      );\n    }\n}\nconst mapStateToProps = (state) => ({\n  login: state.login,\n});\nexport default connect(mapStateToProps, null)(rootFE);\n","/Users/truong.doshi/Coding/React JS/DO_AN/client/src/pages/pagesAdmin/rootAdmin.js",["323","324"],"import React, { Component } from 'react';\nimport { connect } from 'react-redux';\nimport { Switch, Route } from 'react-router-dom';\nimport { routesAdmin } from '../../routes';\nimport SideBar from '../../components/componentsAdmin/sidebar/sidebar';\nimport NavBar from '../../components/componentsAdmin/navbar/navbar';\n\nclass rootAdmin extends Component {\n  constructor(props) {\n    super(props);\n  }\n\n    showContentMenu = (routesAdmin) => {\n      let result = null;\n      if (routesAdmin.length > 0) {\n        result = routesAdmin.map((route, index) => (\n          <Route\n            key={index}\n            path={route.path}\n            exact={route.exact}\n            component={route.main}\n          />\n        ));\n      }\n      return result;\n    }\n\n    render() {\n      if (this.props.login.isLogin == true) {\n        return (\n          <div>\n            <NavBar />\n            <SideBar />\n            <Switch>\n              { this.showContentMenu(routesAdmin) }\n            </Switch>\n          </div>\n        );\n      }\n      this.props.history.push('/login');\n      return null;\n    }\n}\n\nconst mapStateToProps = (state) => ({\n  login: state.login,\n});\n\nexport default connect(mapStateToProps, null)(rootAdmin);\n","/Users/truong.doshi/Coding/React JS/DO_AN/client/src/pages/pagesAuth/rootAuth.js",["325"],"import React, { Component } from 'react';\nimport { 